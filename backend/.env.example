# База данных
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fastapi_backend

# Безопасность
SECRET_KEY=your-secret-key-here  # Замените на результат выполнения: secrets.token_hex(32)
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Настройки приложения
APP_NAME=FastAPI Backend
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=True
API_PREFIX=/api/v1

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000 